{
  "manifest_version": 3,
  "name": "Workday Autofill",
  "description": "Autofills Workday Applications",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo192.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage",
    "nativeMessaging"
  ]
}